<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_cr_pos_send_pending_te" model="ir.cron">
        <field name="name">CR POS FE - Enviar TE pendientes</field>
        <field name="model_id" ref="point_of_sale.model_pos_order"/>
        <field name="state">code</field>
        <field name="code">model._cron_cr_pos_send_pending_te()</field>
        <field name="interval_number">2</field>
        <field name="interval_type">minutes</field>
        <field name="active">True</field>
    </record>

    <record id="ir_cron_cr_pos_check_pending_te_status" model="ir.cron">
        <field name="name">CR POS FE - Consultar estado TE pendientes</field>
        <field name="model_id" ref="point_of_sale.model_pos_order"/>
        <field name="state">code</field>
        <field name="code">model._cron_cr_pos_check_pending_te_status()</field>
        <field name="interval_number">5</field>
        <field name="interval_type">minutes</field>
        <field name="active">True</field>
    </record>
</odoo>
